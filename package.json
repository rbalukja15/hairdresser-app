    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.2",
    "config": "^3.2.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.4",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
  }
}
